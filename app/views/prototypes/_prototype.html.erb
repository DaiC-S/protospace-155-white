<div class="card">
  <%= link_to image_tag(prototype.image, class: :card__img ), root_path%>
  <div class="card__body">
    <%= link_to prototype.prototype_name, root_path, class: :card__title%>
    <p class="card__summary">
      <%= prototype.catchphrase %>
    </p>
    <%= link_to user.name, root_path, class: :card__user %>
  </div>
</div>